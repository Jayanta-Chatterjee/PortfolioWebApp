
<html>
<head>
    <title>MemuPanel</title>
    <link rel="stylesheet" type="text/css" href="~/Content/sidebar.css" />
    <link rel="stylesheet" type="text/css" href=" https://stackpath.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css">
</head>
<body>
    <div>
        <div class="header1">
            <h3>User Panel </h3>
        </div>
        <div class="header2">
            <button type="button" id="btn" name="logout" method="post" action="#"><i class="fa fa-sign-out">Log Out</i></button>
        </div>
        
        <div class="wrap">
            <div class="sidenav">
                <center>
                    <h2 style="color:white;">Dashboard</h2>
                    <button class="dropdown-btn">
                        File
                        <i class="fa fa-caret-down"></i>
                    </button>
                    <div class="dropdown-container">
                        <a href="#">Microsoft Word</a>
                        <a href="#">Microsoft Excel</a>
                        <a href="#">Corel Draw</a>
                    </div>
                    <a href="#contact">About</a>
            </div>
            </center>
        </div>
    </div>
    <div>
        <button type="button" id="btn" name="logout" method="post" action="#"><i class="fa fa-sign-out">Log Out</i></button>
    </div>
    </div>
    <script>
var dropdown = document.getElementsByClassName("dropdown-btn");
var i;
for (i = 0; i < dropdown.length; i++) {
  dropdown[i].addEventListener("click", function() {
  this.classList.toggle("active");
  var dropdownContent = this.nextElementSibling;
  if (dropdownContent.style.display === "block") {
  dropdownContent.style.display = "none";
  } else {
  dropdownContent.style.display = "block";
  }
  });
}
    </script>
</body>
</html>